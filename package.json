{
  "name": "healthlane",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --filter=web",
    "build": "turbo run build --filter=web",
    "lint": "turbo run lint --filter=web",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types --filter=web",
    "vercel-build": "npm install --legacy-peer-deps && next build"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.3",
    "cross-env": "^10.1.0",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "nativewind": "2.0.11",
    "react-native": "0.74.3",
    "react-native-web": "0.19.12"
  },
  "engines": {
    "node": "20.x"
  },
  "resolutions": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.74.3"
  },
  "packageManager": "pnpm@9.7.1"
}